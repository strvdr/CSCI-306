
public class Mascot extends Person{

	private String mySchool;
	private String mascotType;
	
	public Mascot(String name, String occupation, String school, String animal) {
		super(name, occupation);
		mySchool = school;
		mascotType = animal;
	}
	
	public void answerQuestion(){
		System.out.println("I am the mascot for " + mySchool);
	}
	
	
	public void askQuestion() {	
		System.out.println("Did you finish your homework?");
	}

	@Override
	public void whatIDo() {
		super.whatIDo();
		System.out.println("");
	}
}
